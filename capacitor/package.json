{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "description": "An Capacitor project",
  "scripts": {
    "start": "node bin/pre-load-configuration.js && node bin/start.js",
    "android": "node bin/pre-load-configuration.js && node bin/start_android.js",
    "ios": "node bin/pre-load-configuration.js && node bin/start_ios.js",
    "pack": "node bin/pre-load-configuration.js && node bin/pack.js",
    "make": "node bin/pre-load-configuration.js && node bin/make.js"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "plugins": [
      "unused-imports"
    ],
    "rules": {
      "import/no-anonymous-default-export": [
        "off",
        {
          "allowArrowFunction": true
        }
      ],
      "react-hooks/exhaustive-deps": "off",
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/prefer-as-const": "warn",
      "@typescript-eslint/no-empty-object-type": "off",
      "prefer-const": "warn",
      "no-var": "warn",
      "@typescript-eslint/no-unused-vars": "off",
      "unused-imports/no-unused-imports": "warn",
      "unused-imports/no-unused-vars": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
