{
    "name": "my-app",
    "version": "0.1.0",
    "private": true,
    "main": "./dist/index.js",
    "homepage": ".",
    "scripts": {
        "start": "node bin/pre-load-configuration.js && node bin/start.js",
        "pack": "node bin/pre-load-configuration.js && node bin/pack.js",
        "make": "node bin/pre-load-configuration.js && node bin/make.js"
    },
    "eslintConfig": {
        "extends": [
            "eslint:recommended",
            "plugin:@typescript-eslint/recommended"
        ],
        "plugins": [
            "unused-imports"
        ],
        "rules": {
            "import/no-anonymous-default-export": [
                "off",
                {
                    "allowArrowFunction": true
                }
            ],
            "react-hooks/exhaustive-deps": "off",
            "@typescript-eslint/no-explicit-any": "off",
            "@typescript-eslint/prefer-as-const": "warn",
            "@typescript-eslint/no-empty-object-type": "off",
            "prefer-const": "warn",
            "no-var": "warn",
            "@typescript-eslint/no-unused-vars": "off",
            "unused-imports/no-unused-imports": "warn",
            "unused-imports/no-unused-vars": "off"
        }
    },
    "browserslist": {
        "production": [
            "last 1 chrome version"
        ],
        "development": [
            "last 1 chrome version"
        ]
    },
    "build": {
        "extends": null,
        "files": [
            "dist",
            "public",
            "build"
        ],
        "directories": {
            "output": "output"
        },
        "appId": "com.electron.electron",
        "win": {
            "target": "nsis"
        },
        "nsis": {
            "oneClick": false,
            "perMachine": true,
            "deleteAppDataOnUninstall": true,
            "guid": "b5a96e2b-5f4f-4a67-b77a-2c9d7c947242",
            "runAfterFinish": true,
            "allowToChangeInstallationDirectory": true
        },
        "asar": {
            "smartUnpack": true
        },
        "asarUnpack": [
            "public",
            "build",
            "node_modules/@electron/remote",
            "node_modules/node-os-utils"
        ]
    },
    "devDependencies": {
        "@babylonjs/core": "8.30.5",
        "@emotion/react": "11.14.0",
        "@emotion/styled": "11.14.1",
        "@fontsource/roboto": "5.2.8",
        "@fortawesome/free-brands-svg-icons": "6.7.2",
        "@fortawesome/free-solid-svg-icons": "6.7.2",
        "@fortawesome/react-fontawesome": "0.2.2",
        "@mui/icons-material": "7.3.4",
        "@mui/lab": "7.0.1-beta.18",
        "@mui/material": "7.3.4",
        "@mui/x-charts": "8.13.1",
        "@mui/x-data-grid": "8.13.1",
        "@mui/x-data-grid-pro": "8.13.1",
        "@mui/x-date-pickers": "8.12.0",
        "@nestjs/cli": "11.0.10",
        "@rsbuild/core": "1.5.0",
        "@rsbuild/plugin-babel": "1.0.6",
        "@rsbuild/plugin-eslint": "1.1.2",
        "@rsbuild/plugin-node-polyfill": "1.4.2",
        "@rsbuild/plugin-react": "1.4.1",
        "@rsbuild/plugin-type-check": "1.2.4",
        "@tailwindcss/postcss": "4.1.14",
        "@toolpad/core": "0.16.0",
        "@types/node": "24.7.0",
        "@types/node-os-utils": "1.3.4",
        "@types/node-rsa": "1.1.4",
        "@types/qs": "6.14.0",
        "@types/react": "19.0.0",
        "@types/react-dom": "19.0.0",
        "@types/react-virtualized": "9.22.3",
        "@types/wait-on": "5.3.4",
        "@typescript-eslint/eslint-plugin": "8.46.0",
        "@typescript-eslint/parser": "8.46.0",
        "axios": "1.12.2",
        "babel-plugin-macros": "3.1.0",
        "date-fns": "4.1.0",
        "dexie": "4.2.1",
        "electron": "35.2.1",
        "electron-builder": "26.0.12",
        "eslint-plugin-unused-imports": "4.2.0",
        "execa": "5.1.1",
        "get-port": "5.1.1",
        "is-mobile": "5.0.0",
        "js-sha3": "0.9.3",
        "jsencrypt": "3.5.4",
        "mathjs": "14.8.2",
        "mobx-react-use-autorun": "4.0.67",
        "node-rsa": "1.1.1",
        "qs": "6.14.0",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "react-git-info": "2.0.1",
        "react-intl": "7.1.13",
        "react-router-dom": "7.9.4",
        "react-virtualized": "9.22.6",
        "rxjs-websockets": "9.0.0",
        "source-map-support": "0.5.21",
        "tailwindcss": "4.1.14",
        "ts-enum-util": "4.1.0",
        "ts-loader": "9.5.4",
        "tsconfig-paths": "4.2.0",
        "typedjson": "1.8.0",
        "typescript": "5.8.3",
        "typestyle": "2.4.0",
        "vh-check": "2.0.5",
        "wait-on": "9.0.1",
        "web-vitals": "5.1.0",
        "webworker-promise": "0.5.1",
        "windows-locale": "1.1.3"
    },
    "dependencies": {
        "@electron/remote": "2.1.3",
        "linq": "3.2.4",
        "node-os-utils": "1.3.7",
        "rxjs": "7.8.2",
        "rxjs-exhaustmap-with-trailing": "2.1.1",
        "stream-to-string": "1.2.1",
        "tree-kill": "1.2.2",
        "uuid": "13.0.0"
    }
}
