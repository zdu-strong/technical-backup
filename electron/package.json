    "@types/wait-on": "5.3.4",
    "dexie": "4.0.11",
    "electron": "35.2.1",
    "electron-builder": "26.0.12",
    "get-port": "5.1.1",
    "source-map-support": "0.5.21",
    "ts-loader": "9.5.2",
    "tsconfig-paths": "4.2.0",
    "wait-on": "8.0.3"
  },
  "dependencies": {
    "@electron/remote": "2.1.2",
    "linq": "3.2.4",
    "node-os-utils": "1.3.7",
    "rxjs": "7.8.2",
    "stream-to-string": "1.2.1",
    "tree-kill": "1.2.2",
    "uuid": "11.1.0"
  }
}