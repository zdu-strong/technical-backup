{
    "name": "my-app",
    "version": "0.1.0",
    "private": true,
    "homepage": ".",
    "scripts": {
        "start": "node bin/pre-load-configuration.js && node bin/start.js",
        "build": "node bin/pre-load-configuration.js && node bin/build.js"
    },
    "eslintConfig": {
        "extends": [
            "eslint:recommended",
            "plugin:@typescript-eslint/recommended"
        ],
        "plugins": [
            "unused-imports"
        ],
        "rules": {
            "import/no-anonymous-default-export": [
                "off",
                {
                    "allowArrowFunction": true
                }
            ],
            "react-hooks/exhaustive-deps": "off",
            "@typescript-eslint/no-explicit-any": "off",
            "@typescript-eslint/prefer-as-const": "warn",
            "@typescript-eslint/no-empty-object-type": "off",
            "prefer-const": "warn",
            "no-var": "warn",
            "@typescript-eslint/no-unused-vars": "off",
            "unused-imports/no-unused-imports": "warn",
            "unused-imports/no-unused-vars": "off"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@emotion/react": "11.14.0",
        "@emotion/styled": "11.14.1",
        "@fontsource/roboto": "5.2.9",
        "@fortawesome/free-brands-svg-icons": "6.7.2",
        "@fortawesome/free-solid-svg-icons": "6.7.2",
        "@fortawesome/react-fontawesome": "0.2.2",
        "@mui/icons-material": "7.3.6",
        "@mui/lab": "7.0.1-beta.20",
        "@mui/material": "7.3.6",
        "@mui/x-charts": "8.23.0",
        "@mui/x-data-grid": "8.23.0",
        "@mui/x-data-grid-pro": "8.23.0",
        "@mui/x-date-pickers": "8.23.0",
        "@rsbuild/core": "1.5.0",
        "@rsbuild/plugin-babel": "1.0.6",
        "@rsbuild/plugin-eslint": "1.2.0",
        "@rsbuild/plugin-node-polyfill": "1.4.2",
        "@rsbuild/plugin-react": "1.4.2",
        "@rsbuild/plugin-type-check": "1.3.2",
        "@tailwindcss/postcss": "4.1.18",
        "@toolpad/core": "0.16.0",
        "@types/node": "25.0.3",
        "@types/node-rsa": "1.1.4",
        "@types/qs": "6.14.0",
        "@types/react": "19.0.0",
        "@types/react-dom": "19.0.0",
        "@types/react-virtualized": "9.22.3",
        "@typescript-eslint/eslint-plugin": "8.51.0",
        "@typescript-eslint/parser": "8.51.0",
        "axios": "1.13.2",
        "babel-plugin-macros": "3.1.0",
        "bigdecimal.js": "1.3.1",
        "date-fns": "4.1.0",
        "eslint-plugin-unused-imports": "4.3.0",
        "execa": "5.1.1",
        "is-mobile": "5.0.0",
        "js-sha3": "0.9.3",
        "jsencrypt": "3.5.4",
        "linq": "4.0.3",
        "mathjs": "15.1.0",
        "mobx-react-use-autorun": "4.0.70",
        "node-rsa": "1.1.1",
        "qs": "6.14.1",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "react-git-info": "2.0.1",
        "react-intl": "7.1.13",
        "react-router-dom": "7.11.0",
        "react-virtualized": "9.22.6",
        "rxjs": "7.8.2",
        "rxjs-exhaustmap-with-trailing": "2.1.1",
        "rxjs-websockets": "9.0.0",
        "tailwindcss": "4.1.18",
        "ts-enum-util": "4.1.0",
        "typedjson": "1.8.0",
        "typescript": "5.8.3",
        "typestyle": "2.4.0",
        "uuid": "13.0.0",
        "vh-check": "2.0.5",
        "web-vitals": "5.1.0",
        "webworker-promise": "0.5.1",
        "windows-locale": "1.1.3"
    }
}
